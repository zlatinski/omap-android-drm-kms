menuconfig SGX_OMAPLFB
	tristate "SGX omaplfb support"
	depends on OMAP_DSSCOMP
	select OMAPLFB
	default n
	help
	  Support for in-kernel omaplfb. This is an interface between display and the
	  kernel side PowerVR graphics driver.

config OMAPLFB
	bool "OMAP OMAPLFB"
	depends on DSSCOMP
	default y
